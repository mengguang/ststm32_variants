{
  "platform" : "ststm32",
  "build": {
    "cpu": "cortex-m0",
    "extra_flags": "-DSTM32F030x8",
    "f_cpu": "48000000L",
	"variant": "PILL_F030C8",
    "mcu": "stm32f030c8t6"
  },
  "debug": {
    "default_tools": [
      "jlink"
    ],
    "jlink_device": "STM32F030C8",
    "onboard_tools": [
      "stlink"
    ],
    "openocd_board": "st_nucleo_f0",
    "svd_path": "STM32F030.svd"
  },
  "frameworks": [
    "arduino",
    "mbed",
    "stm32cube"
  ],
  "name": "GreenPill STM32F030C8",
  "upload": {
    "maximum_ram_size": 8192,
    "maximum_size": 65536,
    "protocol": "jlink",
    "protocols": [
      "jlink",
      "stlink",
      "blackmagic",
      "mbed"
    ]
  },
  "url": "https://www.st.com/en/microcontrollers-microprocessors/stm32f030c8.html",
  "vendor": "ST"
}
